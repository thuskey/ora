#!/bin/sh
set -e

case "$1" in
    remove|deconfigure)
    	rm -f /usr/share/games/quake3/baseq3/pak0.pk3
    	rm -f /usr/share/games/quake3/baseq3/pak1.pk3
    	rm -f /usr/share/games/quake3/baseq3/pak2.pk3
    	rm -f /usr/share/games/quake3/baseq3/pak3.pk3
    	rm -f /usr/share/games/quake3/baseq3/pak4.pk3
    	rm -f /usr/share/games/quake3/baseq3/pak5.pk3
    	rm -f /usr/share/games/quake3/baseq3/pak6.pk3
    	rm -f /usr/share/games/quake3/baseq3/pak7.pk3
    	rm -f /usr/share/games/quake3/baseq3/pak8.pk3
    ;;

    upgrade)
    ;;

    failed-upgrade)
    ;;
    
    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 0
    ;;
esac

#DEBHELPER#

exit 0
